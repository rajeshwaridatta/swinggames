package rajeshwari.codes.shootBall;

import java.awt.Graphics2D;

public class Blocks {
	public int widthofblock;
	public int heightofblock1;
	public int x1;
	public int y1;

	

	public void draw(Graphics2D g2d, GameJPanel panel){
		g2d.fillRoundRect(x1,y1,widthofblock1,heightofblock1, 50, 50);
	}
}
	
	


