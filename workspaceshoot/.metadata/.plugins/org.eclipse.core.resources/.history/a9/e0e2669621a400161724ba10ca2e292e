package rajeshwari.codes.shootBall;

import java.awt.Graphics2D;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;

public class Tankershooter {
	public int Width=100;
	public int Height=120;
	public int x= 0;
	public int y=480;

	public void draw(Graphics2D g2d, GameJPanel panel) throws IOException{
		BufferedImage image = ImageIO.read(new File("/home/rajeshwari/tanker.png"));
		g2d.drawImage(image, 0, 350, panel);
	}
}
