package rajeshwari.codes.shootBall;

import java.awt.Graphics2D;

public class Blocks {
	public int widthofblock1 = 50;
	public int heightofblock1 = 100;
	public int x1 =  50;
	public int y1 = 100;
	//public int widthofblock2 = 70;
	public int heightofblock2 = 90;
	public int x2 =  200;
	public int y2 = 100;
	
	public void draw(Graphics2D g2d, GameJPanel panel){
	g2d.fillRoundRect(widthofblock1, heightofblock1, x1, y1, 50, 50);
	//g2d.fillRoundRect(widthofblock2, heightofblock2, x2, y2, 50, 50);
	
	}
}
	
	


