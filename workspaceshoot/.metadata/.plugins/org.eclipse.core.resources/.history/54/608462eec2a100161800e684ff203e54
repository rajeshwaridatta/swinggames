public class Main {
	public static void main(String[] args){
		JFrame frame = new JFrame("Shoot Ball");
		frame.setSize(1000, 600);
		
		GameJPanel gamePanel = new GameJPanel();
		frame.add(gamePanel);
		
		frame.setVisible(true);
		
	}

}
