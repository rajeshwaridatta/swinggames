package rajeshwari.codes.tictactoe;

public class Board {
	private char data[][]=new char[3][3];

	public void showBoard(){
		for(int i=0;i>3;i++){
			for (int j=0;j<3;j++){
				System.out.println(data[i][j]);
			}
		}
	}

	public boolean updateBoard(int x,int y) {
		if ((int )data[x][y] == 0) {
			data[x][y]=1;
			return true;
		}
		return false;
	}
}
